type Sprint {
    id: String
    title: String
    perfilCode: String
    project_id: String
    #deliveryDate: Instant
    responsible: String
    #stages: [Stage]
    priority: Int
    notes: String
}

input SprintQuery {
    id: String
    ids: [String]
    title: String
    perfilCode: String
    project_id: String
    responsible: String
    priority: Int
    notes: String
}